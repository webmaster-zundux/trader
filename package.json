{
  "name": "trader",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:prepare": "cp .env.development .env",
    "dev": "react-router dev",
    "build": "react-router build",
    "create-gh-pages-release": "git branch -D gh-pages && git checkout -b gh-pages",
    "build:gh-pages": "cp .env.production.gh-pages .env && npm run build && node scripts/react-router/movePrerenderedPagesOneLevelUpperForGithubPagesOfProject.mjs && node scripts/react-router/renamePrerenderedPages.mjs && rm -rf docs && cp -r build/client docs && cp .nojekyll docs/.nojekyll && git add docs -f",
    "publish-gh-pages-release": "git push --set-upstream origin gh-pages -f",
    "no-exist:start": "react-router-serve ./build/server/index.js",
    "typegen": "react-router typegen",
    "typecheck": "react-router typegen && tsc",
    "print-routes": "react-router routes --json",
    "preview": "vite preview",
    "ci": "npm run typecheck && npm run lint",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:ts": "eslint .",
    "lint:ts:fix": "eslint . --fix",
    "lint:ts:print-config": "eslint --print-config ./src/App.tsx",
    "lint:ts:advanced": "npm run typecheck && tsc -b --noEmit",
    "lint:css": "npx stylelint '**/*.css'",
    "lint:css:fix": "npx stylelint '**/*.css' --fix",
    "dependencies-check": "npx npm-check-updates",
    "dependencies-update": "npx npm-check-updates -u",
    "source:compress": "node scripts/zip-source.mjs",
    "source:compress-all": "node scripts/zip-source-all.mjs",
    "source:decompress": "node scripts/unzip-source.mjs"
  },
  "dependencies": {
    "@react-router/node": "^7.8.0",
    "@vusion/webfonts-generator": "^0.8.0",
    "d3": "^7.9.0",
    "idb-keyval": "^6.2.2",
    "isbot": "^5",
    "nanoid": "^5.1.5",
    "normalize.css": "^8.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.8.0",
    "superjson": "^2.2.2",
    "three": "^0.179.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@material-design-icons/svg": "^0.14.15",
    "@react-router/dev": "^7.8.0",
    "@react-router/serve": "^7.8.0",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@types/css-modules": "^1.0.5",
    "@types/d3": "^7.4.3",
    "@types/node": "^24",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/three": "^0.179.0",
    "@types/vusion__webfonts-generator": "^0.8.6",
    "@vitejs/plugin-react": "^5.0.0",
    "archiver": "^7.0.1",
    "decompress": "^4.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "react-router-devtools": "^5.0.6",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-plugin-svgr": "^4.3.0",
    "vite-svg-2-webfont": "^5.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
